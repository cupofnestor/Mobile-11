package com.mobile.ggc {	import flash.display.MovieClip;		public class ggPlayer extends basePlayer  {				public function ggPlayer(_id:Number, _team:String)		{			super(_id, _team);		}						override public function cardsHandler(e:DealerEvent):void //cards have been dealt to payer for decision		{			trace("Player :: "+team+", Cards Dealt :: "+e.cardViews);		}		override public function issueHandler(e:DealerEvent):void //issue has been dealt for group decision		{			trace("Player :: "+team+", Issue Dealt :: "+e.cardViews);		}					}	}