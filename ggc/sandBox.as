package com.mobile.ggc {		import flash.display.MovieClip;		import flash.events.*;	public class sandBox extends MovieClip {		var game:ggGame;									public function sandBox() {		 	game = new ggGame();						//this loads gamefile, and pushes configs to all classes.			game.gameFile = ("game.xml");						testBtn.addEventListener("btnUp", test);					}				public function test(e:Event):void		{			trace("test");						game.players.thePlayers["Energy"].active = true;		}					}	}