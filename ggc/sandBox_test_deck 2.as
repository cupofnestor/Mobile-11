package com.mobile.ggc {		import flash.display.MovieClip;			public class sandBox extends MovieClip {		var decks:deck = new deck;		var players:Object = {};		var env:Object = {};		var eco:Object = {};		var ene:Object = {};		var pop:Object = {};				var tmpCfg:XML = 		<deckConfig>			<numCards>8</numCards>			<db table="player_decks" path="data/decks.db" />			<players>				<player>Environment</player>				<player>Energy</player>				<player>Economy</player>				<player>Populace</player>			</players>		</deckConfig>;						public function sandBox() {						env.name = "Environment";			env.id = "1";						ene.name = "Energy";			ene.id = "2";						eco.name = "Economy";			eco.id = "3";						pop.name = "Populace";			pop.id = "4";						players.env = env;			players.ene = ene;			players.eco = eco;			players.pop = pop;			// constructor code			decks.config = tmpCfg;			decks.players = players;			decks.addEventListener(DeckEvent.PULLED, cardPulled);			decks.draw("Economy");		}				private function cardPulled(d:DeckEvent):void		{			trace(d);		}					}	}