package com.mobile.ggc{	import flash.events.Event;	public class DBConnectionEvent extends Event 	{		// Event types.				public static const EVENT_DEFAULT:String = "default";		public static const DB_OPEN:String = "opened";		public static const DB_CLOSED:String = "closed";		public static const DB_RETURN:String = "return"; 		public var data:*;				public function DBConnectionEvent(type:String = DBConnectionEvent.EVENT_DEFAULT, bubbles:Boolean = false, cancelable:Boolean = false, _data:* = null) 		{			data = _data;			super(type, bubbles, cancelable);		} 		override public function clone():Event {			// Return a new instance of this event with the same parameters.			return new DBConnectionEvent(type, bubbles, cancelable);		}	}}