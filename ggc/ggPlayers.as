package com.mobile.ggc {		import flash.system.System;		import flash.events.EventDispatcher;	public dynamic class ggPlayers extends EventDispatcher {		private var active:Number = 0;		public var thePlayers:Object = {};				public function ggPlayers() {			// constructor code		}						public function set config(cfg:XML){						for each (var p:XML in cfg.player){				var team:String =  p.toString();				var id:Number = p.@id;				var type:String =  p.@pType;								thePlayers[team] = new ggPlayer(id,team); 				//thePlayers[team] =  (type == "human") ? new ggPlayer(id,team) : new ggNPC(id,team);				thePlayers[team].addEventListener(PlayerEvent.HIT_ME, hitme);			}									System.disposeXML(cfg);			cfg = null;						dispatchEvent(new PlayerEvent(PlayerEvent.PLAYERS_BUILT));		}				private function hitme(p:PlayerEvent):void		{			trace("Player wants a deal");			dispatchEvent(p); //clone event so dealer can see it.		}				public function set dealer(d:ggDealer):void{			for each (var p:ggPlayer in thePlayers){				p.dealer = d;			}		}				public function toObject():Object{			return thePlayers;					}	}	}