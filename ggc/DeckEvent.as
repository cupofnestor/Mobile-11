package com.mobile.ggc{	import flash.events.Event;	public class DeckEvent extends Event 	{		// Event types.				public static const EVENT_DEFAULT:String = "default";		public static const PULLED:String = "pulled"; 		public var cards:*;				public function DeckEvent(type:String = DeckEvent.EVENT_DEFAULT, bubbles:Boolean = false, cancelable:Boolean = false, _data:* = null) 		{			cards = _data;			super(type, bubbles, cancelable);		} 		override public function clone():Event {			// Return a new instance of this event with the same parameters.			return new DeckEvent(type, bubbles, cancelable);		}	}}