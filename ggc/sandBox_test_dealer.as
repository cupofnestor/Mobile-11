package com.mobile.ggc {		import flash.display.MovieClip;			public class sandBox extends MovieClip {		var dealer:ggDealer;						var tmpCfg:XML = 		<config>			<dealer firstname = "joe" />			<deck>				<numCards>8</numCards>				<db table="player_decks" path="data/decks.db" />				<players>					<player>Environment</player>					<player>Energy</player>					<player>Economy</player>					<player>Populace</player>				</players>			</deck>;		</config>				public function sandBox() {						var env:Object = {};			var eco:Object = {};			var ene:Object = {};			var pop:Object = {};											env.name = "Environment";			env.id = "1";						ene.name = "Energy";			ene.id = "3";						eco.name = "Economy";			eco.id = "2";						pop.name = "Populace";			pop.id = "4";			var players:Object = {};			players.env = env;			players.ene = ene;			players.eco = eco;			players.pop = pop;						dealer = new ggDealer();			dealer.config = tmpCfg;			dealer.players = players;			dealer.addEventListener(DealerEvent.CARDS_DEALT,dealtHandler);			dealer.dealTwo("Economy");					}				private function dealtHandler(d:DealerEvent):void		{			trace(d);		}	}	}