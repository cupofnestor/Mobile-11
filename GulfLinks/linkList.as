package com.mobile.GulfLinks {		import flash.display.MovieClip;	import com.adobe.xml.syndication.atom.Link;	import flash.text.StyleSheet;	import flash.html.HTMLLoader;	import flash.events.*;	public class linkList extends MovieClip {		private var window:HTMLLoader;		private var document:XML = <html></html>;		private var head:XML = <head></head>;		private var body:XML = <body></body>;		private var theList:XML;				public function linkList() {			// constructor code			window = new HTMLLoader();			window.paintsDefaultBackground = false;			document.appendChild(head);			addEventListener(Event.ADDED_TO_STAGE, init);		}				private function init(e:Event):void		{			window.width = this.width;			window.height = this.height;			addChild(window);		}						public function linkUpdate(_a:Array):void		{												for each (var _l:link in _a)			{								body.appendChild(_l.htmlLink);			}			document.body=body;			updateDisplay();		}			private function updateDisplay():void		{			var loadStr:String = document.toString();			window.addEventListener(Event.COMPLETE, loadComplete);			window.loadString(loadStr);					}				private function loadComplete(e:Event)		{			trace(e.target);		}				}	}