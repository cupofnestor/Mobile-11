package com.mobile.GulfLinks {		import flash.events.*;	import flash.utils.Dictionary;		public class metaManager extends EventDispatcher {		//the world which will contain all Physical "Items"		var world:thisWorld;		//Item vars		var allItems:Dictionary = new Dictionary();  //dictionary containing ALL items		var worldItems:Dictionary = new Dictionary(); // this array contains all items currently visible.		var focusItem:delItem; //this is the item that currently has focus, or null if no item has focus.				//Link Vars		var list:linkList;		var worldLinks:linkCollection = new linkCollection() //contains all visible links		var allLinks:Array = new Array(); //contains all links				//constructor		public function metaManager(_world:thisWorld, _list:linkList) {			world = _world;			list = _list;		}		public function set feed(_feed:XML):void		{			buildDict(_feed);		}				public function set links(list:XMLList):void		{			for each (var _l:XML in list.link)			{				var _link:link = new link(_l);				allLinks.push(_link);			}			//all links start active for now.			worldLinks.links = allLinks;			//trace("linkManager :: linkList POPULATED");		}				private function buildDict(_feed:XML):void		{						//first, build dictionary of items			for each (var tag:XML in _feed.taglist.tag)			{				var _t:String =tag.toString();				var _f:Number = tag.@freq;				var _i:delItem = new delItem();				_i._tag=_t;				_i._freq=_f;				allItems[_t] = _i; //put delItem ref in the all list				worldItems[_t] = _i;  //put them all in the world list as well			}			//then, list of links			for each (var _l:XML in _feed.links.link)			{				var _link:link = new link(_l);				allLinks.push(_link);			}			//all links start active for now.			worldLinks.links = allLinks;			dispatchEvent(new Event("loadComplete", true));		}				public function testCollection(_s:String):void		{			var _tmp:Object = worldLinks.linksByTag(_s);		}				//searches world links list, returning array of those which contain that tag						//Not really useful		public function addTags(_tags:Array):void		{			for each (var _t:String in _tags)			{				var _item:delItem = allItems[_t];				trace("");				world.addTag(_item);			}		}	}	}