package com.mobile.GulfLinks {	import flash.utils.Dictionary;	public class linkCollection {				private var theLinks:Array = new Array();				public function linkCollection() {			// constructor code		}				public function set links(val:Array):void		{			theLinks = val;		}				public function linksByTag(_t:String):Object		{			var ret:Object = new Object();			var _links:Array = new Array();			for each (var _l:link in theLinks)			{				(_l.containsTag(_t)) ? _links.push(_l) : null;			}			ret.links = new Array();			ret.links = _links;			var _tags:Array = tagsFromLinks(_links);			ret.tags = new Array();			ret.tags = removeDupes(_tags);			return ret;		}				public function tagsFromLinks(_links:Array):Array		{			var ret:Array = new Array();			for each (var _l:link in _links)			{				ret = ret.concat(_l.tagsArray);			}			return ret;		}		private function removeDupes(src:Array):Array		{			var dict:Dictionary = new Dictionary();			var arr:Array = src;			var len:int = arr.length;			for (var i:int = 0; i<len; ++i)			{				var str:String = arr[i] as String;				if (!dict[str])				{					dict[str] = true;				}				else				{					arr.splice(i,1);					i--; len--;				}			}						dict = null;			return arr;		}	}	}