package com.mobile.GulfLinks {	import flash.utils.Dictionary;		public class link {		private var tagList:Dictionary = new Dictionary();		public var id:Number;		public var htmlLink:XML;		public var htmlURL:String;		var linkTag:String;				public function link(_l:XML) {			// constructor code			parseXML(_l);		}				public function set tags(_tags:XMLList)		{			for each (var _t:String in _tags.tag)			{				var tag:Object = new Object();				tag.active = new Boolean(true);				tag.tagName = new String(_t);				tagList[_t] = tag;			}			//trace("link :: tags populated");		}				public function get tagsArray():Array		{			var ret:Array= new Array();			for each (var val:Object in tagList)			{				ret.push(val.tagName);			}			return ret;		}				public function set url(_u:String)		{			htmlURL = _u;		}				public function set desc(_d:XML)		{			var des:String = _d.toString();			var aTag:XML = <a class="header" />;			aTag.appendChild(des);			aTag.@href = htmlURL;			var htmlDetail:XML = <p class="body" />;			htmlDetail.appendChild(_d.@extended);						htmlLink = <div class = "link" />;			htmlLink.appendChild(aTag);			htmlLink.appendChild(htmlDetail);		}				public function containsTag(key:String):Boolean		{			trace("CHECKING TAGLIST "+tagList[key]);			return (tagList[key] == undefined) ? false : true; 		}						public function parseXML(l:XML):void		{			id = l.@id;			url = l.@url;			tags = l.tags;			desc = new XML(l.description);		}	}	}/*<link id="1" url="http://blog.al.com/oil_spill_impact/atom.xml">      <description extended="RSS feed on oil spill">Gulf Oil Spill impact</description>      <tags>        <tag>environment</tag>        <tag>oil</tag>        <tag>pollution</tag>      </tags></link>			*/