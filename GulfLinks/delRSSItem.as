package com.mobile.GulfLinks {		public class delRSSItem {		var title:String;		var created:String;		var url:String;		var date:String;		var description:String;		public var tags:Array = new Array;				public function delRSSItem(item:XML) {			// constructor code			title = item.title;			date = item.pubDate;			url = item.link;			description =  item.description;			for each (var tmpTag:String in item.category){				tags.push(tmpTag);			}		}				public function checkTag(t:String):Boolean		{			var ret:Boolean = false;			for each (var tag:String in tags){				if(t==tag){					ret = true;					break;				}			}			return ret;		}					}	}