package  com.mobile.GulfLinks {	import flash.events.*;	import flash.utils.Dictionary;		///Class to manage wck objects, add functions to add/remove delItems based on arrays passed from stack	public class itemManager extends EventDispatcher {		var world:thisWorld;		var allItems:Dictionary = new Dictionary();  //dictionary containing ALL items		var worldItems:Dictionary = new Dictionary(); // this array contains all items currently visible.		var focusItem:delItem; //this is the item that currently has focus, or null if no item has focus.				public function itemManager(_world:thisWorld) {			// constructor code			world = _world;		}				//set a list of links to build the linkList		public function set links(list:XMLList):void		{			for each (var _l:XML in list.link)			{				var _link:link = new link(_l);				allLinks.push(_link);			}			//trace("linkManager :: linkList POPULATED");		}				//set a list of tags to build dictionaries		public function set tagList(list:XMLList):void		{			buildDict(list);		}				private function buildDict(_list:XMLList):void		{			for each (var tag:XML in _list.tag)			{				var _t:String =tag.toString();				var _f:Number = tag.@freq;				var _i:delItem = new delItem();				_i._tag=_t;				_i._freq=_f;				allItems[_t] = _i; //put delItem ref in the all list				worldItems[_t] = _i;  //put them all in the world list as well			}			trace("itemManager::  ListComplete");			dispatchEvent(new Event("listComplete", true));		}						//Not really useful		public function addTags(_tags:Array):void		{			for each (var _t:String in _tags)			{				var _item:delItem = allItems[_t];				trace("");				world.addTag(_item);			}		}		}	}