package com.mobile.GulfLinks {	import flash.utils.Dictionary;	public class linkManager {		var list:linkList;		var worldLinks:Dictionary = new Dictionary(); //contains all visible links		var allLinks:Array = new Array(); //contains all links			public function linkManager(_list:linkList) {			// constructor code			list = _list;		}				public function set links(list:XMLList):void		{			for each (var _l:XML in list.link)			{				var _link:link = new link(_l);				allLinks.push(_link);			}			//trace("linkManager :: linkList POPULATED");		}	}	}