package com.mobile.GulfLinks {		public class GLCloud {		var clLinks:Array= new Array();		var tags:Array = new Array();				public function GLCloud(_feed:delRSSFeed) {			// constructor code			(_feed.links.length > 0) ? buildCloud(_feed) : report("Feed is empty.",2); 		}				private function buildCloud(_feed:delRSSFeed):void		{			clLinks = _feed.links;			report("Building cloud", 1);			clLinks.forEach(addTags);			report("taglist",1);		}						private function addTags(item:delRSSItem, i:int, a:Array):void		{			for each (var t in item.tags){				(checkTag(t,tags)) ? tags.push(t) : report("Duplicate tag!!",1);			}		}				private function checkTag(_t:String, a:Array):Boolean{			var ret:Boolean = true;			for each (var tag:String in a){				if (tag == _t){					ret = false;					break;				}			}			return ret;					}								private function report(r:String, p:Number=0):void		{			(p>0) ? trace(r) : null;					}	}	}