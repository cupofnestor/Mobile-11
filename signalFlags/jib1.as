package com.mobile.signalFlags {	import flash.events.*;	import flash.display.MovieClip;	import com.nestor.elements.maskedDisplacement;		public class jib1 extends maskedDisplacement {		var lookup:Array=new Array();		var ct:Number = 0;				public function jib1() {			// constructor code			lookup = buildLookup();			super();			speedY = 7; 			channel = 1; 			flapX = 40; 			flapY = 20; 			baseX = 500; // size of noise in x axis			baseY = 100; // size of noise in y axis			octs = 1; // noise functions for noise (smaller = faster)			seed = Math.floor(Math.random() * 22); // random seed for noise			noiseWidth=noiseLength=100;						link(this.jib1_mc, this.displace_mc);			build();			start();		}				override public function loop(evt:Event):void{			ct++;			ct = ct % 360;			speedY = lookup[ct];			super.loop(evt);								}				private function buildLookup():Array		{			var _cos:Array = new Array();			for (var i:Number = 0; i<360;i++){				var rad:Number = i * Math.PI/180;				_cos.push(Math.cos(rad));			}			return _cos;		}	}	}