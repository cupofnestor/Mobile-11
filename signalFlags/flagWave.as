package com.mobile.signalFlags {		import flash.display.MovieClip;	import flash.events.Event;	import caurina.transitions.*;			public class flagWave extends MovieClip {		private var my_initY:Number;		private var my_targetY:Number;		private var my_letter:String;				public function flagWave(l:String) {			// constructor code			my_letter=l;			addEventListener(Event.ADDED_TO_STAGE, init);					}				private function init(e:Event):void		{			//trace("I am a flag, i represent the letter "+my_letter+" ,  my initY= "+my_initY+" , my targetY = "+my_targetY);						this.flag_mc.gotoAndStop(my_letter);			raise();					}				public function set initY(Y:Number):void		{			my_initY = Y;			this.y =  Y;		}				public function set targetY(tY:Number):void		{			my_targetY = tY;		}				public function raise():void		{			//trace ("Raising!");			anim(my_targetY);		}				public function lower():void		{			anim(my_initY);		}				public function anim(tY:Number)		{			//trace ("Animating to "+tY+"!");			Tweener.addTween(this, {				y:tY,				delay: 0,				transition:"easeInOutSine",				time:2,				onComplete:animComplete});		}				private function animComplete():void		{			//trace("::flagWave:: Animation Complete");			dispatchEvent(new Event("flagUp"));		}	}	}						//flag_meta.flag_mc.						/*						*/