package com.mobile.signalFlags {		import flash.display.MovieClip;	import flash.events.Event;		public class flagStatic extends MovieClip {				var _letter:String;				var hPadding:Number = 20;		var sqWide:Number = 300+hPadding;		var rectWide:Number = 520+hPadding;				public var flagWide:Number;				public function set letter(val:String):void		{			switch(val)			{				case "0":					numFlag(1);					break;				case "1":					numFlag(2);					break;				case "2":					numFlag(3);					break;				case "3":					numFlag(4);					break;				case "4":					numFlag(5);					break;				case "5":					numFlag(6);					break;				case "6":					numFlag(7);					break;				case "7":					numFlag(8);					break;				case "8":					numFlag(9);					break;				case "9":					numFlag(10);					break;									case "!":					repFlag(val);					break;				case "@":					repFlag(val);					break;				case "#":					repFlag(val);					break;				case "$":					repFlag(val);					break;									default:					var flVal:String = val.toLowerCase();					this.flag_mc.gotoAndStop(flVal);					flagWide = sqWide;					dispatchEvent(new Event("flagWidth"));				}			}						private function numFlag(n:Number):void			{				flagWide = rectWide;				this.flag_mc.gotoAndStop(n);				dispatchEvent(new Event("flagWidth"));			}						private function repFlag(s:String):void			{				flagWide = rectWide;				this.flag_mc.gotoAndStop(s);				dispatchEvent(new Event("flagWidth"));			}										}	}