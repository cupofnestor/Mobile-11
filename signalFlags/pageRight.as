package com.mobile.signalFlags {		import flash.display.MovieClip;			public class pageRight extends dict_sect {						public function setup(exOne:String,exTwo:String) {			// constructor code						parseFlags1(exOne);			(exTwo == "null") ? null : parseFlags2(exTwo);		}				private function parseFlags2(flMess):void		{			var parsed:Array = parseMulti(flMess);			if (parsed[0] != null){				flags_2.setup(parsed);				//this.two_1.letter = parsedTwo[0];				//this.two_2.letter = parsedTwo[1];				//this.two_mess.text = "\""+parsedTwo[2]+"\"";							}			//this.two_2.letter = flTwo.toLowerCase();					}				private function parseFlags1(flMess):void		{			var parsed:Array = parseMulti(flMess);			if (parsed[0] != null){				flags_1.setup(parsed);				//this.three_1.letter = parsedThree[0];				//this.three_2.letter = parsedThree[1];				//this.three_3.letter = parsedThree[2];				//this.three_mess.text = "\""+parsedThree[3]+"\"";			}		}				private function parseMulti(_tS:String):Array		{			//_ts = "N + C = I am in distress"			var split:Array = _tS.split("=");			var tmpStr:String = split[0];//"N + C"			var tmpArr:String = split[1];//"I am in distress"			tmpStr = findReplace(tmpStr,"+ "," ");   //replace "+" and remove trailing space			var flLetters:Array = tmpStr.split(" "); //split at " '			flLetters = removeSpaces(flLetters);  //Cleanup, so only "N" and "C" return;			flLetters.push(tmpArr); //append the message from tmpArr			return flLetters;		}				private function findReplace(_s:String, f:String,r:String):String		{			var s:String = _s;			s = s.split(f).join(r);			return s;		}				private function removeSpaces(_tA:Array):Array		{			var retArr:Array = new Array();			for each (var s:String in _tA)			{				(s==" " || s=="") ? null : retArr.push(s);			}			return retArr;		}	}	}