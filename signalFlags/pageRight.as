package com.mobile.signalFlags {		import flash.display.MovieClip;			public class pageRight extends dict_sect {						public function setup(_two,_three:String) {			// constructor code			parseTwo(_two);			parseThree(_three);		}				private function parseTwo(flTwo):void		{			var parsedTwo:Array = parseMulti(flTwo);			if (parsedTwo[0] != null){				two_flag.setup(parsedTwo);				//this.two_1.letter = parsedTwo[0];				//this.two_2.letter = parsedTwo[1];				//this.two_mess.text = "\""+parsedTwo[2]+"\"";							}			//this.two_2.letter = flTwo.toLowerCase();					}				private function parseThree(flThree):void		{			var parsedThree:Array = parseMulti(flThree);			if (parsedThree[0] != null){				three_flag.setup(parsedThree);				//this.three_1.letter = parsedThree[0];				//this.three_2.letter = parsedThree[1];				//this.three_3.letter = parsedThree[2];				//this.three_mess.text = "\""+parsedThree[3]+"\"";			}		}				private function parseMulti(_tS:String):Array		{			//_ts = "N + C = I am in distress"			var split:Array = _tS.split("=");			var tmpStr:String = split[0];//"N + C"			var tmpApp:String = split[1];//"I am in distress"			tmpStr = findReplace(tmpStr,"+ "," ");   //replace "+" and remove trailing space			var flLetters:Array = tmpStr.split(" "); //split at " '			flLetters = removeSpaces(flLetters);  //Cleanup, so only "A" and "C" return;			flLetters.push(tmpApp); //append the message from tmpApp			return flLetters;		}				private function findReplace(_s:String, f:String,r:String):String		{			var s:String = _s;			s = s.split(f).join(r);			return s;		}				private function removeSpaces(_tA:Array):Array		{			var retArr:Array = new Array();			for each (var s:String in _tA)			{				(s==" " || s=="") ? null : retArr.push(s);			}			return retArr;		}	}	}