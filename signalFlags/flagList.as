package com.mobile.signalFlags {		import flash.display.MovieClip;	import fl.text.TLFTextField;			public class flagList extends MovieClip {		public var _flagMC:Array = new Array();		public var _flags:Array = new Array();		public var messageText:TLFTextField;				public function set flags(val:Array):void		{			_flags = val;						for (var i:Number = 0; i<_flags.length; i++)			{				_flagMC[i].letter = _flags[i];			}		}				public function set flMessage(val:String):void		{			this.messageText.text = val;		}				public function setup(parsed:Array)		{			flMessage = parsed.pop();			flags = parsed;		}	}	}