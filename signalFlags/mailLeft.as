package com.mobile.signalFlags {		import flash.events.*;			public class mailLeft extends dict_sect {		private var messages:XML;		private var sFromName:String;		private var sFromMail:String;		private var sToMail:String;		public function mailLeft() {			// constructor code			addEventListener(Event.ADDED_TO_STAGE, init);		}				override public function set cfg(val:XML):void		{			super.cfg = val;			messages = new XML(val.messages);		}				public function get data():Object		{			var ret:Object = new Object;			ret.fromName = sFromName;			ret.fromMail = sFromMail;			ret.toMail =  sToMail;			return ret;		}				private function init(e:Event):void		{			fromName.kbActive = true;			fromName.addEventListener("submit", fromNameSubmitted);			this.prompt.htmlText=messages.message.(@type=="name");			this.swingOn();		}				private function fromNameSubmitted(e:Event):void		{			sFromName = e.target.data;		}		private function fromEmailSubmitted(e:Event):void		{					}		private function toEmailSubmitted(e:Event):void		{					}	}	}