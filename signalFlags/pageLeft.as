package com.mobile.signalFlags {		import flash.display.MovieClip;			public class pageLeft extends dict_sect {				public function setup(_flKey:String,_flName:String,_flDef:String):void		{			parseName(_flName);			this.bigFlag.addEventListener("flagWidth", setTextX);			this.bigFlag.letter = _flKey;			this.meaning.text = _flDef;		}				private function setTextX(e:Event):void		{			this.bigFlag.removeEventListener("flagWidth", setTextX);			this.bigLetter.x = this.bigFlag.x + this.bigFlag.flagWide;			this.bigLabel.x = this.bigLetter.x + this.bigLetter.textWidth;			this.sub_field.x = this.bigLetter.x;		}				public function parseName(str:String):void		{			var flSplit:Array = str.split(" ");			this.sub_field.text = (flSplit.length == 1 ) ? "" : "Substitute";			this.bigLetter.text = flSplit[0].substr(0,1);			this.bigLabel.text = flSplit[0].substr(1);		}	}	}