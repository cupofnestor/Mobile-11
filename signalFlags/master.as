package com.mobile.signalFlags {		import flash.display.MovieClip;	import flash.events.KeyboardEvent;	import flash.events.Event;		//remove	public class master extends MovieClip {		var keyboardActive:Boolean=true;		var painting:ship = new ship();				public function master() {			// constructor code			//replace with keyboard dispatch from custom class			addChild(painting);						stage.addEventListener(KeyboardEvent.KEY_DOWN, keyDownHandler);		}						private function keyDownHandler(k:KeyboardEvent):void		{			stage.removeEventListener(KeyboardEvent.KEY_DOWN, keyDownHandler);			//trace("::MASTER:: Keyboard suspended");			(keyboardActive) ? painting.addFlag(k) : null;			painting.addEventListener("resetKeyboard", resetKeyboard);		}						private function resetKeyboard(e:Event):void		{			//trace("::MASTER:: Keyboard reset");			stage.addEventListener(KeyboardEvent.KEY_DOWN, keyDownHandler);		}					}	}