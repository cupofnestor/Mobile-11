package  {	import shapes.*;	import wck.*;	import flash.display.MovieClip;	import flash.events.*;	import caurina.transitions.*;	import Box2DAS.Common.V2;		public class item extends BodyShape {		public var isFocus:Boolean = false;						public override function shapes():void {			circle();		}		public override function create():void {			this.restitution = 0.2;			this.linearDamping = 1;			this.fixedRotation = true;			this.friction = 0.05;			super.create();		}								public function myFocus():void		{				isFocus = true;			this.type = "Animated";			Tweener.addTween(this, {x:0, time:Math.random(), transition:"easeInOutQuart", onComplete: tweenComplete});			Tweener.addTween(this, {y:0, time:Math.random(), transition:"easeInOutQuart", onComplete: tweenComplete});		}				private function tweenComplete():void		{		}				public function myBlur():void		{			isFocus = false;			this.type="Dynamic";		}	}	}