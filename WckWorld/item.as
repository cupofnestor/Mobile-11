package  {	import shapes.*;	import wck.*;	import flash.display.MovieClip;	import flash.events.*;	import caurina.transitions.*;	import Box2DAS.Common.V2;		public class item extends BodyShape {		public var grav:radGrav;						public override function shapes():void {			circle();		}		public override function create():void {			this.restitution = 0.2;			this.linearDamping = 1;			this.fixedRotation = true;			this.friction = 0.05;			super.create();		}								public function addGrav():void		{				trace("adding gravity");			grav=new radGrav();			addChild(grav);			grav.repulse();			this.type = "Animated";			Tweener.addTween(this, {x:0, y:0, scaleX : 2, scaleY:2, time:1, transition:"linear", onComplete: sync});		}				private function sync():void		{			this.dispatchEvent(new Event("recreate"));			trace("recreate me");		}				public function killGrav():void		{			trace("killing gravity");			removeChild(grav);			grav = null;			this.type = "Dynamic";			this.scaleX=this.scaleY=1;			sync();		}				private function randomV():Number		{			var ret:Number = 50*(Math.random() - 0.5);			return (ret > 0) ? 50+ret : -50+ret;		}	}	}