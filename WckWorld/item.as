package  {	import caurina.transitions.*;	import shapes.Circle	import flash.display.MovieClip;	import flash.events.*;		public class item extends Circle {		public var grav:radGrav;				public function item() {			// constructor code						this.addEventListener(Event.ADDED_TO_STAGE, init);		}				public function init(e:Event):void		{				this.linearDamping = 2;			this.fixedRotation = true;			this.friction = 0.1;			this.linearVelocityX = randomV();			this.linearVelocityY = randomV();		}				public function addGrav():void		{				trace("adding gravity");			grav=new radGrav();			addChild(grav);												this.type = "Static";						circle(22);					}		public function killGrav():void		{			trace("killing gravity");			removeChild(grav);			grav = null;			this.type = "Dynamic";		}				private function randomV():Number		{			var ret:Number = 50*(Math.random() - 0.5);			return (ret > 0) ? 50+ret : -50+ret;		}	}	}